{
  "name": "Spotify Steramer",
  "description": "Simple streamer for Telegram Bio",
  "repository": "https://github.com/XuliGan4eg2006/Stream-music-from-Spotify/",
  "logo": "https://www.pngitem.com/pimgs/m/148-1487614_spotify-logo-small-spotify-logo-transparent-hd-png.png",
  "keywords": ["Spotify", "express", "Telegram", "Bio", "Python", "Opensurse"],
"env": {
    "redirect_uri": {
      "description": "Your app name",
      "value": "https://YOURAPPNAME.herokuapp.com"
    },
    "client_id": {
      "description": "Your app config https://developer.spotify.com/dashboard/applications/",
      "required": true
    },
    "client_secret": {
      "description": "Your app config https://developer.spotify.com/dashboard/applications/",
      "required": true
    },
    "spotiusername": {
      "description": "Your name in Spotify",
      "required": true
    },
    "api_id": {
      "description": "my.telegram.org",
      "required": true
    },
    "api_hash": {
      "description": "my.telegram.org",
      "required": true
    },
    "def_status": {
      "description": "Your default bio in Telegram",
      "required": true
    },
      "bot_token": {
      "description": "BotFather in Telegram",
      "required": true
    },
      "telegram_id": {
      "description": "Your id in Telegram",
      "required": true
    }
  }
}
